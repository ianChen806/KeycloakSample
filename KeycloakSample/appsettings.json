{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Authentication": {
    "Authority": "http://localhost:8080/realms/MyRealm",
    "ClientId": "MyClient",
    "ClientSecret": "",
    "ResponseType": "code",
    "CallbackPath": "/auth/callback",
    "SignedOutCallbackPath": "/auth/signedOut",
    "Scope": [
      "openid",
      "profile",
      "email"
    ]
  }
}
